{
  "name": "vue-mini-social-network",
  "version": "1.0.0",
  "description": "A mini-social-network created with VueJS!!",
  "main": "app.js",
  "scripts": {
    "start": "nodemon app.js",
    "dev": "webpack -d --watch",
    "build": "webpack -p && eslint public/js/src/ --fix",
